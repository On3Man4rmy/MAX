<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root type="javafx.scene.layout.HBox"
         xmlns:fx="http://javafx.com/fxml/1"
         xmlns="http://javafx.com/javafx/8">
    <Label fx:id="lblPlayerName">
        <font>
            <Font size="22.0" />
        </font>
      <HBox.margin>
         <Insets right="20.0" />
      </HBox.margin>
    </Label>
    <Label fx:id="lblPlayerScore" text="0">
        <font>
            <Font size="22.0" />
        </font>
        <HBox.margin>
            <Insets right="10.0" />
        </HBox.margin>
    </Label>
    <Label fx:id="lblSelected">
        <font>
            <Font size="22.0" />
        </font>
        <HBox.margin>
            <Insets right="20.0" />
        </HBox.margin>
    </Label>
</fx:root>
